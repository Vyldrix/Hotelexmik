a = int(input("area: "))
b = int(input("semilla: "))
#c = int(input("altura: "))
op = "s"
render = (a*a)+1
C1 = b % 10
C2 = (b // 10) % 10
C3 = (b // 100) % 10
C4 = (b // 1000) % 10
C5 = (b // 10000) % 10
C6 = (b // 100000) % 10
C7 = (b // 1000000) % 10
C8 = (b // 10000000) % 10
Unidad = C1
Decena = C2
Centena = C3
Milena = C4
Decmil = C5
Cenmil = C6
Milonea = C7
Decmillo = C8
seed = b
Unidad = Unidad % render
Decena = Decena % render
Centena = Centena % render
Milena= Milena% render
Decmil= Decmil% render
Cenmil= Cenmil% render
while True: 
    #calculo
    

    Unidad += C1
    Decena += C2
    Centena += C3
    Milena += C4
    Decmil += C5
    Cenmil += C6
    Milonea += C7
    Decmillo += C8
        
    if Unidad > render:
        Decena += 1
        Unidad=Unidad% render
    if Decena > render:
        Centena += 1
        Decena = Decena % render
    if Centena > render:
        Milena += 1
        Centena = Centena % render
    if Milena > render:
        Decmil += 1
        Milena=Milena% render
    if Decmil > render:
        Cenmil += 1
        Decmil=Decmil% render
    if Cenmil > render:
        Milonea += 1
        Cenmil= Cenmil% render
    if Milonea > render:
         Milonea = Milonea % render
         Decmillo += Decmillo + 1
        
        #mensaje de salida
    seed += b
    Decmillo = Decmillo % render   
    print("Resultado")
    print(Decmillo)
    print("Calculo:")
    print(seed)
    print("Numerium:")
    Milonea =  Milonea % render
    Cenmil= Cenmil % render
    Decmil = Decmil % render
    Milena =  Milena % render 
    Centena =  Centena % render
    Decena =  Decena % render
    Unidad =  Unidad % render
    print(Decmillo, Milonea, Cenmil, Decmil, Milena, Centena, Decena, Unidad)
    op = input("Â¿continuar? s/n: ")
    
    if op == "n":
        break
